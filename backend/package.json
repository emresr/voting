{
   "name": "rest-fastify",
   "version": "1.0.0",
   "license": "MIT",
   "scripts": {
      "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
      "test": "jest",
      "build": "tsc",
      "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
      "lint": "eslint . --ext .ts",
      "lint-and-fix": "eslint . --ext .ts --fix",
      "prepare": " npm run build"
   },
   "dependencies": {
      "@prisma/client": "2.25.0",
      "fastify": "3.18.0",
      "fastify-cors": "^6.0.1"
   },
   "devDependencies": {
      "prisma": "2.25.0",
      "@types/node": "^15.12.5",
      "ts-node-dev": "^1.1.6",
      "typescript": "^4.3.4",
      "@types/jest": "^26.0.23",
      "@typescript-eslint/eslint-plugin": "^4.28.0",
      "@typescript-eslint/parser": "^4.28.0",
      "eslint": "^7.29.0",
      "jest": "^27.0.3",
      "prettier": "^2.3.1",
      "ts-jest": "^27.0.2"
   }
}
